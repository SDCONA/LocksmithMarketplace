{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --force",
  "framework": "vite",
  "rewrites": [
    { "source": "/hub", "destination": "/index.html" },
    { "source": "/deals", "destination": "/index.html" },
    { "source": "/messages", "destination": "/index.html" },
    { "source": "/account", "destination": "/index.html" },
    { "source": "/admin", "destination": "/index.html" },
    { "source": "/marketplace", "destination": "/index.html" },
    { "source": "/listing", "destination": "/index.html" },
    { "source": "/settings", "destination": "/index.html" },
    { "source": "/profile", "destination": "/index.html" },
    { "source": "/help", "destination": "/index.html" },
    { "source": "/seller-listings", "destination": "/index.html" },
    { "source": "/promote", "destination": "/index.html" },
    { "source": "/contact", "destination": "/index.html" },
    { "source": "/privacy", "destination": "/index.html" },
    { "source": "/terms", "destination": "/index.html" },
    { "source": "/marketplace-profile", "destination": "/index.html" },
    { "source": "/saved-items", "destination": "/index.html" },
    { "source": "/saved-marketplace-listings", "destination": "/index.html" },
    { "source": "/saved-deals", "destination": "/index.html" },
    { "source": "/archived-listings", "destination": "/index.html" },
    { "source": "/retailer-dashboard", "destination": "/index.html" },
    { "source": "/my-retailer-deals", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}